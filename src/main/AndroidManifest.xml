<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.santiagoosorio.addressbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:label="@string/Principal"/>
        <activity android:name=".RegistroActivity"
            android:label="@string/Registrarse"/>
        <activity android:name=".PerfilActivity"
            android:label="@string/Perfil"/>
        <activity android:name=".LogginActivity"
            android:label="@string/Loggin"/>
        <activity android:name=".Main2Activity" />
        <activity android:name=".InformacionActivity"></activity>
    </application>

</manifest>